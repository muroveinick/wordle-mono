{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "noEmit": false,
    "moduleResolution": "node",
    "allowImportingTsExtensions": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "target": "ES2020"
  },
  "include": [
    "**/*.ts", "../../types.d.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
    
  ]
}